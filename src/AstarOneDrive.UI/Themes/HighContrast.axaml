<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
        <ResourceDictionary>

  <Color x:Key="Color_Background">#FF000000</Color>
  <Color x:Key="Color_Foreground">#FFFFFFFF</Color>
  <Color x:Key="Color_Primary">#FFFFFF00</Color>
  <Color x:Key="Color_CardBackground">#FF000000</Color>
  <Color x:Key="Color_TerminalBackground">#FF000000</Color>

  <SolidColorBrush x:Key="AppBarBackground" Color="{DynamicResource Color_Primary}" />
  <SolidColorBrush x:Key="DrawerBackground" Color="#FF000000" />
  <SolidColorBrush x:Key="CardBackground" Color="{DynamicResource Color_CardBackground}" />
  <SolidColorBrush x:Key="TerminalBackground" Color="{DynamicResource Color_TerminalBackground}" />
  <SolidColorBrush x:Key="TerminalBarBackground" Color="#FF000000" />
  <SolidColorBrush x:Key="TerminalStatusBackground" Color="#FF000000" />

  <SolidColorBrush x:Key="ThemeForegroundBrush" Color="{DynamicResource Color_Foreground}" />
  <SolidColorBrush x:Key="ThemeBackgroundBrush" Color="{DynamicResource Color_Background}" />

</ResourceDictionary>

</Styles>