<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Styles.Resources>

  <Color x:Key="Color_Background">#FFF5F5F7</Color>
  <Color x:Key="Color_Foreground">#FF202020</Color>
  <Color x:Key="Color_Primary">#FF005A9E</Color>
  <Color x:Key="Color_CardBackground">#FFFFFFFF</Color>
  <Color x:Key="Color_TerminalBackground">#FF111111</Color>

  <SolidColorBrush x:Key="AppBarBackground" Color="{DynamicResource Color_Primary}" />
  <SolidColorBrush x:Key="DrawerBackground" Color="#FFE0E0E5" />
  <SolidColorBrush x:Key="CardBackground" Color="{DynamicResource Color_CardBackground}" />
  <SolidColorBrush x:Key="TerminalBackground" Color="{DynamicResource Color_TerminalBackground}" />
  <SolidColorBrush x:Key="TerminalBarBackground" Color="#FF222222" />
  <SolidColorBrush x:Key="TerminalStatusBackground" Color="#FF151515" />

  <SolidColorBrush x:Key="ThemeForegroundBrush" Color="{DynamicResource Color_Foreground}" />
  <SolidColorBrush x:Key="ThemeBackgroundBrush" Color="{DynamicResource Color_Background}" />

    </Styles.Resources>
</Styles>